<h1>THEIR FEED!</h1>

<table class='tbl-feed'>
  <% byebug %>
  <tr class='column-titles'>
    <th>type</th><th>user_name</th><th>commit_location</th><th id='repo_url'>repo_url</th><th>commit_id</th><th>created_at</th>
  </tr>
  <% @recents.each do |commit| %>
  <tr>
    <td><%= commit.type %></td><td><%= commit.username %></td><td><%= commit.repo_name %></td><td><%= link_to commit.repo_name, commit.repo %></td><td><%= commit.id %></td><td><%= commit.created_at %></td>
  </tr>
<% end %></table>
