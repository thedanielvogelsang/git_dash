<h1>MY FEED!</h1>

<table><% @recents.each do |commit| %>
  <tr>
    <th>type</th><th>repo_name</th><th>repo_url</th><th>commit_id</th>
  </tr>
  <tr>
    <td><%= commit[:type] %></td><td><%= link_to commit[:repo][:name], "http://github.com/#{commit[:repo][:name]}" %></td><td><%= link_to commit[:repo][:name], commit[:repo][:url] %></td><td><%= commit[:id] %></td>
  </tr>
<% end %></table>
